#                                                                    -*-perl-*-
$description = "List manipulaton functions\n";

$details = "Try various uses of rest, chop
results.\n";

run_make_test('
res = $(rest a b c)

all:
	@echo res=$(res)',
              "",
              "res=b c\n"
             );

run_make_test('
res = $(chop a b c)

all:
	@echo res=$(res)',
                  "",
                  "res=a b\n"
);
1;
